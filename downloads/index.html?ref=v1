    function videoInfo(src){
        if(src){
            $(".video").show();
            $(".section_container").hide();
            $("#videoID").attr("src",src).attr("width","100%");
            $("#mp4-url2").html(src);
        }

    }
    var query = location.href.split('#');
    if(query[1] && query[1].indexOf("url=") >= 0){
        $.get("/load_url",query[1],function(res){
            videoInfo(res)
        });
    }
